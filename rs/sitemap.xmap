			<!-- resource reading URL mapping -->

			<map:match pattern="lov*" type="wildcard">
				<map:act type="JDoPssAction">
					<map:aggregate element="aggregation">
						<map:part src="cocoon://xsp-generator.system.constantes.xsp"/>
						<map:part src="{generator}"/>
					</map:aggregate>
					<map:transform src="resources/stylesheets/html/system.lov.xsl" type="xslt"/>
					<map:serialize type="html"/>
				</map:act>
			</map:match>


			<!-- JavaScript contents URL mapping -->

			<map:match pattern="resources/menu[*].js" type="wildcard">
				<map:act type="http-header-action">
					<map:parameter name="Last-Modified" value=""/>
				</map:act>
				<map:generate type="JMenuGenerator"/>
				<map:transform src="resources/stylesheets/js/menu.js.xsl" type="xslt"/>
				<map:serialize type="js"/>
			</map:match>

			<map:match pattern="resources/tree[*].js" type="wildcard">
				<map:act type="http-header-action">
					<map:parameter name="Last-Modified" value=""/>
				</map:act>
				<map:generate type="JTreeGenerator"/>
				<map:transform src="resources/stylesheets/js/tree.js.xsl" type="xslt"/>
				<map:serialize type="js"/>
			</map:match>



			<map:match pattern="resources/nls.strings.*.js" type="wildcard">
				<map:act type="http-header-action">
					<map:parameter name="Last-Modified" value=""/>
				</map:act>
				<map:generate type="JGlobalStringsGenerator"/>
				<map:transform src="resources/stylesheets/js/nls.strings.js.xsl" type="xslt"/>
				<map:serialize type="js"/>
			</map:match>

			<map:match pattern="v_*/**/*.js" type="wildcard">
				<map:read type="resource" src="{2}/{3}.js" mime-type="text/javascript"/>
			</map:match>

			<map:match pattern="js/*.js" type="wildcard">
				<map:read type="resource" src="js/{1}.js" mime-type="text/javascript"/>
			</map:match>

			<map:match pattern="v_*/**/*.css" type="wildcard">
				<map:read type="resource" src="{2}/{3}.css" mime-type="text/css"/>
			</map:match>
			
			<map:match pattern="css/*.css" type="wildcard">
				<map:read type="resource" src="css/{1}.css" mime-type="text/css"/>
			</map:match>


			<map:match pattern="v_*/**.*" type="wildcard">
				<map:read type="resource" src="{2}.{3}"/>
			</map:match>
		

			<!-- CSS contents -->

			<map:match pattern="resources/styles[*].css" type="wildcard">
				<map:act type="http-header-action">
					<map:parameter name="Last-Modified" value=""/>
				</map:act>
				<map:generate type="JSkinGenerator"/>
				<map:transform src="resources/stylesheets/css/styles.css.xsl" type="xslt"/>
				<map:serialize type="css"/>
			</map:match>


			<!-- pss resources -->  
			<map:match pattern="file/**.*" type="wildcard">
				<map:read type="file" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="business_resource/**.*" type="wildcard">
				<map:read type="business_resource" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="pss_icon/**.*" type="wildcard">
				<map:read type="pss_icon" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="publicweb/*" type="wildcard">
				<map:read type="publicweb" src="{1}"/>
			</map:match>


			<map:match pattern="markPending/**" type="wildcard">
				<map:read type="publicweb" src="{1}"/>
			</map:match>

			<map:match pattern="skin/**.*" type="wildcard">
				<map:read type="skin_resource" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="pss_data/**.*" type="wildcard">
				<map:read type="pssdata_resource" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="pssdata_resource/**.*" type="wildcard">
				<map:read type="pssdata_resource" src="{1}.{2}"/>
			</map:match>
			<map:match pattern="layout/**.*" type="wildcard">
				<map:read type="layout" src="{1}.{2}"/>
			</map:match>



			<!-- image resources -->
			<map:match pattern="work/**.png" type="wildcard">
				<map:read type="resource" mime-type="image/jpg" src="work/{1}.png"/>
			</map:match>
			<map:match pattern="work/**.jpg" type="wildcard">
				<map:read type="resource" mime-type="image/jpg" src="work/{1}.jpg"/>
			</map:match>
			<map:match pattern="work/**.gif" type="wildcard">
				<map:read type="resource" mime-type="image/jpg" src="work/{1}.gif"/>
			</map:match>


			<!-- generic resources -->
			<map:match pattern="**.*" type="wildcard">
				<map:read type="resource" src="{1}.{2}"/>
			</map:match>

			<map:match pattern="*" type="wildcard">
				<map:redirect-to uri="do" />
			</map:match>
			<map:handle-errors>
				<map:generate type="exception"/>
				<map:transform src="resources/stylesheets/html/system.error2html.xsl" type="xslt"/>
				<map:serialize type="html-nocompress"/>
			</map:handle-errors>


		</map:pipeline>
	</map:pipelines>

