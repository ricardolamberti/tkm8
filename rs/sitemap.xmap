<?xml version="1.0" encoding="UTF-8"?>
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">

	<!-- =========================== Components ================================ -->
	<map:components> 

		<map:transformers default="xslt">
			<map:transformer logger="sitemap.transformer.xslt" name="xslt" pool-grow="2" pool-max="32" pool-min="8" src="pss.www.platform.transformer.JWebTransformer">
				<use-request-parameters>false</use-request-parameters>
				<use-browser-capabilities-db>false</use-browser-capabilities-db>
				<use-deli>false</use-deli>
			</map:transformer>
			<map:transformer name="writeDOMsession" src="org.apache.cocoon.transformation.WriteDOMSessionTransformer"/>
		</map:transformers>

		<map:serializers default="html">
			<map:serializer name="xml" logger="sitemap.serializer.xml" mime-type="text/xml" src="org.apache.cocoon.serialization.XMLSerializer"/>

			<map:serializer name="html" logger="sitemap.serializer.html" mime-type="text/html" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedHTMLSerializer">
				<doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="html-decoded" logger="sitemap.serializer.html" mime-type="text/html" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedHTMLSerializer">
				<doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-5</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="html-nocompress" logger="sitemap.serializer.html" mime-type="text/html" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.HTMLSerializer">
				<doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-5</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="graph" logger="sitemap.serializer.html" mime-type="text/html" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedHTMLSerializer">
				<doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="map" logger="sitemap.serializer.html" mime-type="text/html" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedHTMLSerializer">
				<doctype-public>-//W3C//DTD HTML 4.01 Transitional//EN</doctype-public>
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="excel" logger="sitemap.serializer.xls" mime-type="application/vnd.ms-excel" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.XMLSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>


			<map:serializer name="none" logger="sitemap.serializer.xls" mime-type="application/vnd.ms-excel" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.TextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="json" logger="sitemap.serializer.json" mime-type="application/json" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.TextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>UTF-8</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="json-compress" logger="sitemap.serializer.json" mime-type="application/json" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedTextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>UTF-8</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="mobile" logger="sitemap.serializer.json" mime-type="application/json" pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JCompressedTextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>UTF-8</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>
			
			<map:serializer name="csv" logger="sitemap.serializer.csv" mime-type="text/csv" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.TextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="js" logger="sitemap.serializer.js" mime-type="text/javascript" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.TextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>

			<map:serializer name="css" logger="sitemap.serializer.css" mime-type="text/css" pool-grow="4" pool-max="32" pool-min="4" src="org.apache.cocoon.serialization.TextSerializer">
				<buffer-size>1024</buffer-size>
				<encoding>ISO-8859-1</encoding>
				<omit-xml-declaration>true</omit-xml-declaration>
				<indent>false</indent>
			</map:serializer>
<!--
			<map:serializer name="report" logger="sitemap.serializer.report"  mime-type="application/pdf"  pool-grow="4" pool-max="32" pool-min="4" src="pss.www.platform.content.compression.JReportSerializer">
				<user-config>WEB-INF/fop-config.xml</user-config>
			</map:serializer>

-->

